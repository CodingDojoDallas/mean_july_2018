<% if(quotes.length >= 0) { %>
   <% for (var x in quotes) { %>
      <h3><%= quotes[x].name + "- " + quotes[x].createdAt %></h3>
      <h3><%= "'" + quotes[x].text + "'" %></h3>
   <% } %>
<% } %>

